{
  "name": "automationexercise-cypress",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "test": "cypress run",
    "test:headed": "cypress open",
      "test:ci": "cypress run --browser chrome --reporter mochawesome --reporter-options reportDir=cypress/reports,overwrite=false,html=true,json=true",
      "merge:reports": "mochawesome-merge cypress/reports/mochawesome*.json > cypress/reports/mochawesome.json && marge cypress/reports/mochawesome.json -f report -o cypress/reports/",
    "lint": "eslint . --ext .ts,.js"
  },
  "devDependencies": {
    "cypress": "^12.10.0",
    "typescript": "^4.9.5",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@faker-js/faker": "^8.0.2"
  }
}
